{
  "name": "sql-formatter-gui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.2",
    "@types/react": "^16.9.48",
    "@types/react-dom": "^16.9.8",
    "browserslist": "^4.14.0",
    "bulma": "^0.9.0",
    "bulma-tooltip": "^3.0.2",
    "gpm": "^3.0.1",
    "node-pretty-data": "^0.40.1",
    "node-sass-chokidar": "^1.5.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.3",
    "serve": "^11.3.2",
    "sql-formatter": "^2.3.3",
    "typescript": "^4.0.2"
  },
  "scripts": {
    "start": "npm run build && npm run serve",
    "serve": "serve -s build",
    "start-dev": "npm-run-all -p watch-css start-ts",
    "start-ts": "react-scripts start",
    "build": "npm run build-css && react-scripts build",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "prettier-stylelint": "^0.4.2",
    "stylelint-config-rational-order": "0.1.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
