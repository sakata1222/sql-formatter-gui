{
  "name": "sql-formatter-gui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.12",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "browserslist": "^4.6.3",
    "bulma": "^0.7.5",
    "bulma-tooltip": "^2.0.2",
    "node-pretty-data": "^0.40.1",
    "node-sass-chokidar": "^1.3.5",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "^3.0.1",
    "serve": "^11.0.2",
    "sql-formatter": "^2.3.3",
    "typescript": "^3.5.2"
  },
  "scripts": {
    "start": "npm run build && npm run serve",
    "serve": "serve -s build",
    "start-dev": "npm-run-all -p watch-css start-ts",
    "start-ts": "react-scripts start",
    "build": "npm run build-css && react-scripts build",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "prettier-stylelint": "^0.4.2",
    "stylelint-config-rational-order": "0.1.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
